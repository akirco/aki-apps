{
    "version": "0.10.1",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.10.1/uv-x86_64-pc-windows-msvc.zip",
            "hash": "64c297ef1cd8e3a50966dee20cbe039564cd59e41186e0d1dd38fa4e627fc285"
        },
        "32bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.10.1/uv-i686-pc-windows-msvc.zip",
            "hash": "c4e989d479f9fc229302345a64f272be3c249d5fff4a2e722aa3d73c381fb303"
        },
        "arm64": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.10.1/uv-aarch64-pc-windows-msvc.zip",
            "hash": "9644d0e37c41c19aa65137a928bf6fad78dc887f820202c0cfcf010cceb416a0"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe",
        "uvw.exe"
    ],
    "env_set": {
        "UV_CACHE_DIR": "$persist_dir\\caches",
        "UV_PYTHON_CACHE_DIR": "$dir\\python\\caches",
        "UV_INSTALL_DIR": "$dir\\bin",
        "UV_PYTHON_INSTALL_DIR": "$dir\\python\\versions",
        "UV_PYTHON_BIN_DIR": "$dir\\python\\bin",
        "UV_TOOL_BIN_DIR": "$dir\\tools\\bin",
        "UV_TOOL_DIR": "$dir\\tools"
    },
    "env_add_path": [
        "bin",
        "python\\versions",
        "python\\bin",
        "tools\\bin"
    ],
    "persist": [
        "bin",
        "python",
        "tools"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            },
            "32bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-i686-pc-windows-msvc.zip"
            },
            "arm64": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-aarch64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
